<div id="user-list">
  <% if (users.length !=0) { %>
  <% users.forEach(user=> { %>
      <a href="/users/<%= user.username %>/posts">
        <div class="user-card">
          <div class="user-image">
            <img src="<%= user.user_image_path %>" alt="User Image">
          </div>
          <div class="input-spacer"></div>
          <div class="user-details">
            <span id="display-name"><%= user.user_display_name %></span>
            <span id="username">@<%= user.username %></span>
            <span id="bio"><%= user.user_bio %></span>
          </div>
        </div>
      </a>
      <div class="card-spacer"></div>
    <% }); %>
      <% } else { %>
        <div class="no-posts-container">
          <div class="unknown-container">
              <img class="sticker" src="/images/error_ramen.png" alt="unknown page sticker">
              <p>Hmm, looks like there are no users yet!</p>
          </div>
      </div>
      <% } %>
</div>